{
  "name": "org-github",
  "version": "1.0.0",
  "description": "GitHub organization .github repository template",
  "placeholders": {
    "{{ORG_NAME}}": "Organization name (e.g., AlaweinLabs)",
    "{{ORG_DESCRIPTION}}": "Organization description",
    "{{ORG_DOMAIN}}": "Organization domain for security emails"
  },
  "files": [
    "profile/README.md",
    "ISSUE_TEMPLATE/bug_report.md",
    "ISSUE_TEMPLATE/feature_request.md",
    "PULL_REQUEST_TEMPLATE.md",
    "SECURITY.md",
    "workflow-templates/ci.yml",
    "workflow-templates/ci.properties.json"
  ],
  "usage": "Create a repo named '.github' in your org and copy these files"
}
