{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./core", "./modules", "./monitoring", "./optimization", "./quality", "./scripts"],
    "includePattern": ".+\\.(js|jsx)$",
    "excludePattern": "(^|\\/)node_modules(\\/|$)"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/jsdoc",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
