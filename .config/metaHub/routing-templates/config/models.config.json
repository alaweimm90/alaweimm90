  "providers": {
    "openai": {
      "name": "OpenAI",
      "baseUrl": "https://api.openai.com/v1",
      "authType": "bearer",
      "rateLimitStrategy": "token_bucket",
      "retryStrategy": "exponential_backoff",
      "compliance": ["GDPR", "SOC2"],
      "regions": ["north_america", "europe"],
      "supportedFeatures": ["streaming", "function_calling", "fine_tuning"]
    },
    "anthropic": {
      "name": "Anthropic",
      "baseUrl": "https://api.anthropic.com",
      "authType": "bearer",
      "rateLimitStrategy": "fixed_window",
      "retryStrategy": "linear_backoff",
      "compliance": ["GDPR", "SOC2", "HIPAA"],
      "regions": ["north_america"],
      "supportedFeatures": ["streaming", "vision"]
    },
    "gemini": {
      "name": "Google Gemini",
      "baseUrl": "https://generativelanguage.googleapis.com",
      "authType": "bearer",
      "rateLimitStrategy": "sliding_window",
      "retryStrategy": "exponential_backoff",
      "compliance": ["GDPR", "SOC2"],
      "regions": ["north_america", "europe", "asia_pacific"],
      "supportedFeatures": ["multimodal", "streaming", "function_calling"]
    },
    "mistral": {
      "name": "Mistral AI",
      "baseUrl": "https://api.mistral.ai",
      "authType": "bearer",
      "rateLimitStrategy": "token_bucket",
      "retryStrategy": "exponential_backoff",
      "compliance": ["GDPR", "SOC2"],
      "regions": ["europe"],
      "supportedFeatures": ["streaming", "function_calling", "fine_tuning"]
    },
    "local": {
      "name": "Local LLM",
      "baseUrl": "http://localhost:8000",
      "authType": "none",
      "rateLimitStrategy": "unlimited",
      "retryStrategy": "none",
      "compliance": ["GDPR"],
      "regions": ["global"],
      "supportedFeatures": ["offline", "fine_tuning"]
    },
    "openrouter": {
      "name": "OpenRouter",
      "baseUrl": "https://openrouter.ai/api/v1",
      "authType": "bearer",
      "rateLimitStrategy": "dynamic",
      "retryStrategy": "exponential_backoff",
      "compliance": ["GDPR"],
      "regions": ["global"],
      "supportedFeatures": ["routing", "streaming"]
    },
    "together": {
      "name": "Together AI",
      "baseUrl": "https://api.together.xyz",
      "authType": "bearer",
      "rateLimitStrategy": "token_bucket",
      "retryStrategy": "exponential_backoff",
      "compliance": ["GDPR"],
      "regions": ["global"],
      "supportedFeatures": ["streaming", "fine_tuning"]
    }
  },

  "modelGroups": {
    "premium": {
      "description": "Highest quality models for critical tasks",
      "models": ["GPT-4", "Claude-3-Opus", "Claude-3.5-Sonnet"],
      "maxCostPerRequest": 5.0,
      "targetLatency": 3000
    },
    "balanced": {
      "description": "Good quality with reasonable cost",
      "models": ["GPT-3.5-Turbo", "Claude-3-Sonnet", "Gemini-Pro", "Mistral-Large"],
      "maxCostPerRequest": 1.0,
      "targetLatency": 2000
    },
    "budget": {
      "description": "Cost-effective models for simple tasks",
      "models": ["Local-LLM", "OpenRouter-Budget", "Together-AI-Budget", "Claude-3-Haiku"],
      "maxCostPerRequest": 0.1,
      "targetLatency": 1000
    },
    "multimodal": {
      "description": "Models supporting images and other media",
      "models": ["Gemini-Pro", "Gemini-Pro-Vision"],
      "maxCostPerRequest": 1.0,
      "targetLatency": 2000
    },
    "code": {
      "description": "Specialized for programming tasks",
      "models": ["GPT-4", "Claude-3-Opus", "GPT-3.5-Turbo", "Claude-3.5-Sonnet"],
      "maxCostPerRequest": 3.0,
      "targetLatency": 2500
    }
  },

  "regionalOverrides": {
    "europe": {
      "preferredProviders": ["mistral", "openai", "anthropic"],
      "gdprCompliantOnly": true,
      "dataLocalizationRequired": true
    },
    "asia_pacific": {
      "preferredProviders": ["gemini", "openai"],
      "lowLatencyPriority": true
    },
    "north_america": {
      "preferredProviders": ["openai", "anthropic", "gemini"],
      "highReliabilityPriority": true
    }
  },

  "dynamicUpdates": {
    "enabled": true,
    "updateInterval": 3600,
    "sources": [
      {
        "name": "provider_apis",
        "type": "api",
        "endpoints": ["openai", "anthropic", "gemini"],
        "updateFields": ["rateLimits", "pricing", "availability"]
      },
      {
        "name": "performance_monitoring",
        "type": "metrics",
        "updateFields": ["latency", "reliability", "qualityScore"]
      }
    ]
  },

