    {
      "type": "graph",
      "title": "Container CPU (cAdvisor)",
      "gridPos": {"x":0,"y":8,"w":24,"h":8},
      "targets": [{"expr": "sum(rate(container_cpu_usage_seconds_total[5m])) by (container_label_com_docker_compose_service)"}]
    }
  ]
